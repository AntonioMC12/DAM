package Ejercicio2;

public class Consumidor extends Thread {
	private Cubeta cubeta;

	public Consumidor(Cubeta cubeta) {
		this.cubeta = cubeta;
	}

	public void run() {
		int valor = 0;
		for (int i = 0; i < 5; i++) {
			valor = cola.get();// recoge un numero
			System.out.println("Consume: " + valor);
		}
	}

}
