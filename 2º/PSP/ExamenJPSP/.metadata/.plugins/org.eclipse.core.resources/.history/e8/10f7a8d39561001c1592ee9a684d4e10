package Ejercicio2;

public class Consumidor extends Thread {
	private Cubeta cubeta;

	public Consumidor(Cubeta cubeta) {
		this.cubeta = cubeta;
	}

	public void run() {
		this.cubeta.sacaAgua(generateNumber());
	}

	public int generateNumber() {
		double value = (Math.random() * 7) + 1;
		return (int) value;

	}

}
